grammar SQLSelect
  include SQLRowSupport
  include SQLWhereCondition
  include SQLLimit
  include SQLOrderByClause
  include SQLFromClause
  include SQLSelectClause

  rule select_expression
    select 
    from 
    one_or_more_spaces where_condition {
      def eval
        where = where_condition.eval

        CachedModel::SelectExpression.new({
          :select => select.eval,
          :from   => from.eval,
          :where  => where
        })
      end
    }
  end
end