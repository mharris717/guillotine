grammar SQLSelect
  rule primitives
    backtick_string / string / number
  end

  rule backtick_string
    "`" string "`" {
      def eval(backtick_string_class=BackTickString)
        backtick_string_class.new(text_value.gsub("`", ""))
      end
    }
  end

  rule string
    [a-zA-Z_]+ {
      def eval
        text_value
      end
    }
  end

  rule number
    [0-9]+ {
      def eval
        text_value.to_i
      end
    }
  end
end